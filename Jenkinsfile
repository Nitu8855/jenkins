pipeline {

	agent {
				
				label {
							
						label 'built-in'
						customWorkspace '/mnt/pipeline25'
				
				}
	
	}
	
	stages {
				
				stage ('mkdir') {
				
								steps {
											echo "Creating folder za"
											sh "mkdir ji"
									                sh "touch 4 8 3"
								}
				}
	
	}
	
}
